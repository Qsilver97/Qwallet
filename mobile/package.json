    "axios": "^1.6.8",
    "events": "^3.3.0",
    "expo": "~50.0.14",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "^2.9.3",
    "native-base": "^3.4.28",
    "nodejs-mobile-react-native": "^18.17.7",
    "normalize-css-color": "^1.0.2",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
