{
  "name": "qwallet",
  "version": "0.1.5",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "setadb": "set ANDROID_ADB_SERVER_PORT=5554",
    "clean": "cd android && gradlew clean && cd ..",
    "build": "expo build:android -t app-bundle",
    "serverlog": "adb logcat | grep NODEJS-MOBILE | sed 's/.*NODEJS-MOBILE://'",
    "dev": "nodemon",
    "release": "cd android && gradlew assembleRelease"
  },
  "dependencies": {
    "@expo/metro-runtime": "^3.1.3",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-native-fontawesome": "^0.3.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-clipboard/clipboard": "^1.14.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@reduxjs/toolkit": "^2.2.3",
    "axios": "^1.6.8",
    "events": "^3.3.0",
    "expo": "~50.0.14",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "^2.9.3",
    "native-base": "^3.4.28",
    "nodejs-mobile-react-native": "^18.17.7",
    "normalize-css-color": "^1.0.2",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.73.6",
    "react-native-collapsible": "^1.6.1",
    "react-native-fs": "^2.20.0",
    "react-native-localization": "^2.3.2",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.30.1",
    "react-native-svg": "14.1.0",
    "react-native-swiper": "^1.6.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.19.10",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-toastify": "^10.0.5",
    "tailwind-react-native-classnames": "^1.5.1",
    "websocket": "^1.0.34",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-dom": "^18.2.24",
    "metro-react-native-babel-transformer": "^0.77.0",
    "nodemon": "^3.1.0",
    "react-native-svg-transformer": "^1.3.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.1.3"
  },
  "nodemonConfig": {
    "watch": [
      "nodejs-assets/nodejs-project/**/*"
    ],
    "exec": "npm run android"
  },
  "private": true
}
